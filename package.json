{
  "name": "youu-mobile",
  "version": "1.0.0",
  "description": "A React OOTB Mobile Framework",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "cross-env PORT=8181 craco start",
    "build": "cross-env craco build",
    "analyze": "cross-env ANALYZE=true craco build",
    "lint": "eslint --ext .js src mock tests",
    "lint:fix": "eslint --fix --ext .js src mock tests",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@types/webpack-env": "^1.16.2",
    "ahooks": "^2.9.2",
    "css-animation": "^2.0.0",
    "lodash-decorators": "^6.0.0",
    "path-to-regexp": "^6.1.0",
    "qs": "^6.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.15.4",
    "react-intl-universal": "^2.4.6",
    "react-router-dom": "^5.3.0",
    "typescript": "^4.4.3",
    "umi-request": "^1.3.0",
    "zarm": "^2.9.7"
  },
  "devDependencies": {
    "@craco/craco": "^6.0.0",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.1",
    "cross-env": "^7.0.0",
    "enzyme": "^3.1.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^3.1.0",
    "husky": "^4.3.0",
    "less": "^3.8.1",
    "less-loader": "^7.0.0",
    "lint-staged": "^7.2.0",
    "mocker-api": "^2.7.4",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpackbar": "^5.0.0-3"
  },
  "lint-staged": {
    "**/*.{js,jsx,sass}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ]
}
